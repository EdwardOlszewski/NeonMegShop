cart.itemsPrice) + Number(cart.taxPrice)).toFixed(2)\r\n\r\n  // useEffect hook to do something after render\r\n  useEffect(() => {\r\n    if (success) {\r\n      history.push(`/payment/${order._id}`)\r\n      dispatch({ type: USER_DETAILS_RESET })\r\n      dispatch({ type: ORDER_CREATE_RESET })\r\n    }\r\n  }, [history, success, dispatch, order])\r\n\r\n  // Function to place order on submit\r\n  const placeOrderHandler = () => {\r\n    dispatch(\r\n      createOrder({\r\n        orderItems: cart.cartItems,\r\n        shippingAddress: cart.shippingAddress,\r\n        paymentMethod: cart.paymentMethod,\r\n        itemsPrice: cart.itemsPrice,\r\n        shippingPrice: cart.shippingPrice,\r\n        taxPrice: cart.taxPrice,\r\n        totalPrice: cart.totalPrice,\r\n        billingDetails: {\r\n          name: '',\r\n          email: '',\r\n          address: {\r\n            city: '',\r\n            line1: '',\r\n            state: '',\r\n            postal_code: '',\r\n          },\r\n        },\r\n      })\r\n    )\r\n  }\r\n\r\n  return (\r\n    <div style={{ width: '90%', margin: 'auto', textAlign: 'center' }}>\r\n      <Meta title='Review Order' />\r\n      <CheckoutSteps step1 step2 step3 />\r\n\r\n      <div className='content-div'>\r\n        <Row style={{ marginTop: '2rem' }}>\r\n          <Col sm={12} md={12} lg={12} xl={7} style={{ marginBottom: '2rem' }}>\r\n            <Card style={{ border: 'none' }}>\r\n              <ListGroup variant='flush'>\r\n                <ListGroup.Item className='bg-color'>\r\n                  <h2>Order Items</h2>\r\n                  {cart.cartItems.length === 0 ? (\r\n                    <Message>Your cart is empty</Message>\r\n                  ) : (\r\n                    <ListGroup variant='flush'>\r\n                      <ListGroup.Item className='bg-color'></ListGroup.Item>\r\n                      {cart.cartItems.map((item, index) => (\r\n                        <ListGroup.Item key={index} className='bg-color'>\r\n                          <Row>\r\n                            <Col md={2}>\r\n                              <Image\r\n                                src={item.image}\r\n                                alt={item.name}\r\n                                fluid\r\n                                rounded\r\n                              />\r\n                            </Col>\r\n\r\n                            <Col md={5} style={{ marginTop: '2rem' }}>\r\n                              <Link\r\n                                className='links'\r\n                                to={`/product/${item.product}`}\r\n                                style={{ fontSize: '130%' }}\r\n                              >\r\n                                {item.name}\r\n                              </Link>\r\n                            </Col>\r\n                            <Col md={5}>\r\n                              <p\r\n                                style={{ marginTop: '2rem', fontSize: '130%' }}\r\n                              >\r\n                                {item.qty} x ${item.price} = $\r\n                                {item.qty * item.price}{' '}\r\n                              </p>\r\n                            </Col>\r\n                          </Row>\r\n                        </ListGroup.Item>\r\n                      ))}\r\n\r\n                      <ListGroup.Item className='bg-color'></ListGroup.Item>\r\n                    </ListGroup>\r\n                  )}\r\n                </ListGroup.Item>\r\n              </ListGroup>\r\n            </Card>\r\n          </Col>\r\n\r\n          <Col\r\n            sm={12}\r\n            md={12}\r\n            lg={8}\r\n            xl={4}\r\n            style={{ textAlign: 'center', margin: 'auto' }}\r\n          >\r\n            <Card style={{ border: 'none' }}>\r\n              <ListGroup variant='flush'>\r\n                <ListGroup.Item className='bg-color'>\r\n                  <h2>Shipping</h2>\r\n          