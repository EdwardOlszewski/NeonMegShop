port { Link } from 'react-router-dom'\r\nimport { useDispatch, useSelector } from 'react-redux'\r\nimport {\r\n  Row,\r\n  Col,\r\n  Image,\r\n  ListGroup,\r\n  Card,\r\n  Button,\r\n  Form,\r\n  Container,\r\n} from 'react-bootstrap'\r\nimport Rating from '../components/Rating'\r\nimport Message from '../components/Message'\r\nimport Loader from '../components/Loader'\r\nimport { listProductDetails } from '../actions/productActions'\r\nimport { createRequest } from '../actions/requestActions'\r\nimport { PRODUCT_CREATE_REVIEW_RESET } from '../constants/productConstants'\r\nimport Meta from '../components/Meta'\r\nimport ReviewModal from '../components/ReviewModal'\r\n\r\nconst ProductScreen = ({ history, match }) => {\r\n  const [qty, setQty] = useState(1)\r\n  const [rating, setRating] = useState(0)\r\n  const [comment, setComment] = useState('')\r\n\r\n  const ID = match.params.id\r\n\r\n  const dispatch = useDispatch()\r\n\r\n  const productDetails = useSelector((state) => state.productDetails)\r\n  const { loading, error, product } = productDetails\r\n\r\n  const userLogin = useSelector((state) => state.userLogin)\r\n  const { userInfo } = userLogin\r\n\r\n  const productReviewCreate = useSelector((state) => state.productReviewCreate)\r\n  const {\r\n    success: successProductReview,\r\n    loading: loadingProductReview,\r\n    error: errorProductReview,\r\n  } = productReviewCreate\r\n\r\n  useEffect(() => {\r\n    if (\r\n      !product._id ||\r\n      product._id !== match.params.id ||\r\n      successProductReview\r\n    ) {\r\n      dispatch(listProductDetails(match.params.id))\r\n      dispatch({ type: PRODUCT_CREATE_REVIEW_RESET })\r\n    }\r\n  }, [dispatch, match, product._id, successProductReview])\r\n\r\n  const addToCartHandler = () => {\r\n    history.push(`/cart/${match.params.id}?qty=${qty}`)\r\n  }\r\n\r\n  const requestItemHandler = () => {\r\n    if (userInfo) {\r\n      dispatch(createRequest(product.name))\r\n    } else {\r\n      history.push('/login')\r\n    }\r\n  }\r\n\r\n  return (\r\n    <Container>\r\n      {loading ? (\r\n        <Loader />\r\n      ) : error ? (\r\n        <Message variant='danger'>{error}</Message>\r\n      ) : (\r\n        <>\r\n          <Meta title={product.name} />\r\n          <Card className='card-content'>\r\n            <Row>\r\n              <Col sm={12} md={12} lg={12} xl={5}>\r\n                <Image\r\n                  className='product-image'\r\n                  src={product.image}\r\n                  alt={product.name}\r\n                  fluid\r\n                />\r\n              </Col>\r\n\r\n              <Col\r\n                md={12}\r\n                lg={9}\r\n                xl={5}\r\n                style={{ margin: 'auto', textAlign: 'center' }}\r\n              >\r\n                <ListGroup variant='flush'>\r\n                  <ListGroup.Item className='bg-color'>\r\n                    <h5>{product.name}</h5>\r\n                  </ListGroup.Item>\r\n                  <ListGroup.Item className='bg-color'>\r\n                    <Rating\r\n                      value={product.rating}\r\n                      text={`${product.numReviews}  ${' '} reviews`}\r\n                    />\r\n                  </ListGroup.Item>\r\n\r\n                  <Card>\r\n                    <ListGroup variant='flush'>\r\n                      <ListGroup.Item className='bg-color'>\r\n                        <Row>\r\n                          <Col>Price:</Col>\r\n                          <Col>\r\n                            <strong>\r\n                              $\r\n                              {(Math.round(product.price * 100) / 100).toFixed(\r\n                                2\r\n                              )}\r\n                            </strong>\r\n                          </Col>\r\n                        </Row>\r\n                      </ListGroup.Item>\r\n\r\n                      <ListGroup.Item className='bg-color'>\r\n                        <Row>\r\n                          <Col>Status:</Col>\r\n                          <Col>\r\n                            {product.countInStock > 0 ? (\r\n                              <strong>In Stock</strong>\r\n                            ) : (\r\n                              <strong>Out Of Stock</strong>\r\n                            )}\r\n                          </Col>\r\n                        </Row>\r\n                      </ListGroup.Item>\r\n\r\n                      <ListGroup.Item className='bg-color'>\r\n                        <Row>\r\n                          <Col sm={6} xl={6}>\r\n                            Description:\r\n                          </Col>\r\n                          <Col sm={6}>\r\n                            <strong>{product.description}</strong>\r\n                          </Col>\r\n                        </Row>\r\n                      </ListGroup.Item>\r\n                      {product.countInStock <= 0 ? (\r\n                        <ListGroup.Item className='bg-color'>\r\n                          <Button\r\n                            onClick={requestItemHandler}\r\n                            className='btn-block'\r\n                            type='button'\r\n                            disabled\r\n                          >\r\n                            Out Of Stock!\r\n                          </Button>\r\n                        </ListGroup.Item>\r\n                      ) : (\r\n                        <>\r\n                          <ListGroup.Item className='bg-color'>\r\n                            <Row>\r\n                              <Col style={{ marginTop: '8px' }}>Qty:</Col>\r\n                              <Col>\r\n                                <Form.Control\r\n                                  as='select'\r\n                                  value={qty}\r\n                                  onChange={(e) => setQty(e.target.value)}\r\n                                >\r\n                                  {[...Array(product.countInStock).keys()].map(\r\n                                    (x) => (\r\n                                      <option key={x + 1} value={x + 1}>\r\n                                        {x + 1}\r\n                                      </option>\r\n                                    )\r\n                                  )}\r\n                                </Form.Control>\r\n                              </Col>\r\n                            </Row>\r\n                          </ListGroup.Item>\r\n\r\n                          <ListGroup.Item className='bg-color'>\r\n                            <Button\r\n                              onClick={addToCartHandler}\r\n                              className='bt'\r\n                              style={{ width: '100%' }}\r\n                              type='button'\r\n                              disabled={product.countInStock === 0}\r\n                            >\r\n                              Add To Cart\r\n                            </Button>\r\n                          </ListGroup.Item>\r\n                        </>\r\n                      )}\r\n                    </ListGroup>\r\n                  </Card>\r\n                </ListGroup>\r\n              </Col>\r\n            </Row>\r\n          </Card>\r\n\r\n          <Row style={{ marginBottom: '2rem', marginTop: '1rem' }}>\r\n            <Col xs={12} sm={12} md={12} lg={12} xl={12}>\r\n              <Card className='card-content'>\r\n                <h2>Reviews</h2>\r\n\r\n                {product.reviews.length === 0 && <Message>No Reviews</Message>}\r\n                <ListGroup variant='flush'>\r\n                  <ListGroup.Item\r\n                    style={{\r\n                      backgroundColor: 'rgb(255, 208, 249)',\r\n                      border: 'none',\r\n                      marginBottom: '-13px',\r\n                    }}\r\n                  ></ListGroup.Item>\r\n                  {product.reviews.map((review) => (\r\n                    <ListGroup.Item key={review._id} className='review-txt'>\r\n                      <Rating value={review.rating} />\r\n\r\n                      <p>{review.comment}</p>\r\n                      <p>\r\n                        written by {review.name},{' '}\r\n                        {review.createdAt.substring(0, 10)}\r\n                      </p>\r\n                    </ListGroup.Item>\r\n                  ))}\r\n                  <ListGroup.Item\r\n                    style={{\r\n                      backgroundColor: 'rgb(255, 208, 249)',\r\n                      border: 'none',\r\n                      marginTop: '2px',\r\n                    }}\r\n                  ></ListGroup.Item>\r\n                </ListGroup>\r\n\r\n                <ListGroup variant='flush' style={{ border: 'none' }}>\r\n                  {loadingProductReview && <Loader />}\r\n                  {errorProductReview && (\r\n                    <Message variant='danger'>{errorProductReview}</Message>\r\n                  )}\r\n                  {userInfo ? (\r\n                    <ReviewModal ID={ID} />\r\n                  ) : (\r\n                    <Message>\r\n                      Please <Link to='/login'>sign in</Link> to write a review{' '}\r\n                    </Message>\r\n                  )}\r\n                </ListGroup>\r\n              </Card>\r\n            </Col>\r\n          </Row>\r\n        </>\r\n      )}\r\n    </Container>\r\n  )\r\n}\r\n\r\nexport default ProductScreen\r\n\r\n/*\r\n\r\n     <Row>\r\n            <Col md={1} xl={2}></Col>\r\n            <Col sm={12} md={12} lg={12} xl={9} style={{ marginTop: '3rem' }}>\r\n              <ListGroup>\r\n                <ListGroup.Item\r\n                  style={{ marginTop: '2rem' }}\r\n                  className='bg-color'\r\n                >\r\n                  <h2>Write a Customer Review</h2>\r\n                  {successProductReview && (\r\n                    <